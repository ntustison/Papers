	Journal = {J Neurotrauma},
	Journal-Full = {Journal of neurotrauma},
	Mesh = {Adult; Aged; Anisotropy; Brain; Brain Injuries; Cognition Disorders; Data Interpretation, Statistical; Diffusion Magnetic Resonance Imaging; Female; Humans; Image Processing, Computer-Assisted; Male; Middle Aged; Retrospective Studies},
