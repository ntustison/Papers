	Pages = {2508--2519},
	Pii = {awm216},
	Pmid = {17872928},
	Timestamp = {2011.02.08},
	Title = {White matter integrity and cognition in chronic traumatic brain injury: a diffusion tensor imaging study.},
	Url = {http://dx.doi.org/10.1093/brain/awm216},
