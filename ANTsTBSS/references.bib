	Pages = {209--219},
	Pmid = {8661285},
	Timestamp = {2011.02.08},
	Title = {Microstructural and physiological features of tissues elucidated by quantitative-diffusion-tensor {MRI}.},
	Volume = {111},
	Year = {1996}}

