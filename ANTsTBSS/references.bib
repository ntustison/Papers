	Pii = {S1053-8119(10)01586-7},
	Pmid = {21146617},
	Timestamp = {2011.02.08},
	Title = {The effect of template selection on diffusion tensor voxel-based analysis results.},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2010.12.005},
