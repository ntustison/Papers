	Pages = {546--554},
	Pii = {S1053-8119(05)00095-9},
	Pmid = {15907311},
	Timestamp = {2011.02.09},
	Title = {The effect of filter size on {VBM} analyses of {DT-MRI} data.},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2005.02.013},
